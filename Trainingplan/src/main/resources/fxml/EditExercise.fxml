<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="850.0" minWidth="900.0" style="-fx-background-color: #4189FB; -fx-background-color: linear-gradient(to top , #5050FF, #4189FB);" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.hdmstuttgart.trainingsapp.src.controller.EditExerciseController">
   <children>
      <VBox alignment="CENTER" layoutY="22.0" prefHeight="75.0" prefWidth="500.0" spacing="40.0" AnchorPane.bottomAnchor="30.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="40.0">
         <children>
            <HBox fx:id="editExerciseSettings" spacing="10.0">
               <children>
                  <Button fx:id="weekplan" alignment="TOP_CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#switchTo" style="-fx-background-color: #406CF4;">
                     <graphic>
                        <ImageView>
                           <image>
                              <Image url="@../images/Zurückpfeil.png" />
                           </image>
                        </ImageView>
                     </graphic>
                     <HBox.margin>
                        <Insets left="150.0" />
                     </HBox.margin>
                  </Button>
                  <Label alignment="CENTER" prefWidth="400.0" text="Auf der Stelle laufen" textAlignment="CENTER" textFill="WHITE">
                     <font>
                        <Font name="System Font" size="36.0" />
                     </font>
                  </Label>
               </children></HBox>
            <HBox alignment="CENTER">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER" maxWidth="400.0" minHeight="50.0" prefHeight="50.0" prefWidth="311.0" spacing="35.0">
               <children>
                  <Label fx:id="exerciseCountReps" alignment="CENTER" contentDisplay="CENTER" layoutX="100.0" text="Anzahl der Wiederholungen" textFill="WHITE">
                     <font>
                        <Font size="24.0" />
                     </font>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                  </Label>
                  <TextField alignment="CENTER_RIGHT" prefHeight="25.0" prefWidth="46.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" maxWidth="400.0" minHeight="50.0" spacing="35.0">
               <children>
                  <Label fx:id="exerciseCountSet" alignment="CENTER" contentDisplay="CENTER" layoutX="50.0" text="Anzahl der Sätze" textFill="WHITE">
                     <font>
                        <Font size="24.0" />
                     </font>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                  </Label>
                  <TextField alignment="CENTER_RIGHT" prefHeight="25.0" prefWidth="46.0">
                     <HBox.margin>
                        <Insets left="105.0" />
                     </HBox.margin></TextField>
               </children>
            </HBox>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </VBox>
   </children>
</AnchorPane>
